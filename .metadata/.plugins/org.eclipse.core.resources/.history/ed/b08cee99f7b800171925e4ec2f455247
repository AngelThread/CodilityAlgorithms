package com.codility.algorithms;

public class CyclicRotation {

	public static void main(String[] args) {

		int sampleArray[] = { 3, 8, 9, 7, 6 };
		int current = 0;
		int before = 0;
		for (int i = 0; i < sampleArray.length; i++) {
			if (i != 0 && i + 1 != sampleArray.length) {
				current = sampleArray[i];
				sampleArray[i] = before;
				before = current;
			}

		}

	}

}
