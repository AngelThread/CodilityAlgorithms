package com.codility.algorithms;

public class PermCheck {
	public int solution(int[] A) {
        // write your code in Java SE 8
        
        int sum = (A.length * (A.length + 1)) / 2;
        
        for(int i=0; i<A.length; i++){
            
            sum -= A[i];
            
            
            }
            
            return sum == 0 ? 1:0;
    }
}
